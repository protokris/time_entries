{
  "env": {
    "browser": true,
    "node": true,
    "jquery": true,
    "es6": false
  },
  "globals": {
    "React": true
   },
  "ecmaFeatures": {
    "jsx": true,
    "binaryLiterals": false,
    "blockBindings": false,
    "defaultParams": false,
    "forOf": false,
    "generators": false,
    "objectLiteralComputedProperties": false,
    "objectLiteralDuplicateProperties": false,
    "objectLiteralShorthandMethods": false,
    "objectLiteralShorthandProperties": false,
    "octalLiterals": false,
    "regexUFlag": false,
    "regexYFlag": false,
    "templateStrings": false,
    "unicodeCodePointEscapes": false
  },
  "plugins": [
    "react"
   ],
  "parserOptions": {
    "ecmaVersion": 5,
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
      "comma-dangle": 1,
      "no-dupe-keys": 1,
      "no-extra-semi": 1,
      "no-unreachable": 1,

      "dot-notation": 1,
      "eqeqeq": 1,
      "camelcase": 1,
      "eol-last": 1,
      "no-mixed-spaces-and-tabs": 1,
      "semi-spacing": 1,
      "semi": 1
   }
}
